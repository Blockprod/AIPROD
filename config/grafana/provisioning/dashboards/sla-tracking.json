{
  "dashboard": {
    "title": "AIPROD - SLA & Error Tracking",
    "description": "SLA compliance, error tracking, and reliability metrics",
    "tags": ["aiprod", "sla", "reliability", "errors"],
    "timezone": "browser",
    "schemaVersion": 39,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Pipeline Success Rate (24h)",
        "type": "gauge",
        "gridPos": { "x": 0, "y": 0, "w": 8, "h": 8 },
        "targets": [
          {
            "expr": "((rate(user_jobs_completed_total[24h]) * 86400) / (rate(pipeline_executions_total[24h]) * 86400 + 0.0001)) * 100",
            "legendFormat": "Success %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 100,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 95 },
                { "color": "green", "value": 99.5 }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "P95 Latency vs SLA Target (300s)",
        "type": "gauge",
        "gridPos": { "x": 8, "y": 0, "w": 8, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(pipeline_duration_seconds_bucket[1h]))",
            "legendFormat": "P95 Duration",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "max": 300,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 180 },
                { "color": "red", "value": 300 }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Uptime SLA (24h)",
        "type": "stat",
        "gridPos": { "x": 16, "y": 0, "w": 8, "h": 8 },
        "targets": [
          {
            "expr": "avg_over_time(up{job=\"fastapi-api\"}[24h]) * 100",
            "legendFormat": "Uptime %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 99 },
                { "color": "green", "value": 99.9 }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Error Distribution",
        "type": "piechart",
        "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum by (reason) (rate(render_failures_total[1h]))",
            "legendFormat": "{{ reason }}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Render Failures Timeline",
        "type": "graph",
        "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(render_failures_total[5m])",
            "legendFormat": "{{ reason }} ({{ stage }})",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Failures/sec" }
        ]
      },
      {
        "id": 6,
        "title": "QA Gate Acceptance Rate",
        "type": "gauge",
        "gridPos": { "x": 0, "y": 16, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "((rate(qa_gate_acceptance_total{result=\"passed\"}[1h]) / (rate(qa_gate_acceptance_total[1h]) + 0.0001)) * 100)",
            "legendFormat": "Acceptance Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 80 },
                { "color": "green", "value": 95 }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Agent Timeout Rate",
        "type": "graph",
        "gridPos": { "x": 12, "y": 16, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(ai_agent_calls_total{status=\"timeout\"}[5m])",
            "legendFormat": "{{ agent_type }} timeouts",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Timeouts/sec" }
        ]
      },
      {
        "id": 8,
        "title": "API Response Time Distribution",
        "type": "graph",
        "gridPos": { "x": 0, "y": 24, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(api_response_time_seconds_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(api_response_time_seconds_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(api_response_time_seconds_bucket[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yaxes": [
          { "label": "Response Time (seconds)" }
        ]
      },
      {
        "id": 9,
        "title": "HTTP Error Rate by Endpoint",
        "type": "piechart",
        "gridPos": { "x": 12, "y": 24, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum by (endpoint) (rate(api_response_time_seconds_bucket{status_code=~\"5..\"}[5m]))",
            "legendFormat": "{{ endpoint }}",
            "refId": "A"
          }
        ]
      }
    ]
  }
}
